{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#imports": [
        ".nuxt/imports.d.ts"
      ],
      "#services/*": [
        "./services/*"
      ],
      "#models/*": [
        "./models/*"
      ],
      "#utils/*": [
        "./utils/*"
      ],
      "#stores/*": [
        "./stores/*"
      ],
      "#loaders/*": [
        "./loaders/*"
      ],
      "#components/*": [
        "./components/*"
      ],
      "#layouts/*": [
        "./layouts/*"
      ]
    },
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "@pinia/nuxt"
    ]
  }
}
